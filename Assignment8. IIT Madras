        "from sklearn.linear_model import Ridge\n",
        "\n",
        "x=df[['Mileage']]\n",
        "y=df['Price']\n",
        "LR = LinearRegression()\n",
        "LR.fit(x,y)\n",
        "\n",
        "ridge = Ridge(alpha=1)\n",
        "ridge.fit(x,y)\n",
        "print('R^2 score for this equation:',ridge.score(x,y))"
